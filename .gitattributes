# Auto detect text files and perform LF normalization
* text=auto
*.sql linguist-detectable=true
*.sql linguist-language=sql

*.js    eol=lf
*.jsx   eol=lf
*.json  eol=lf

# Custom for Visual Studio
*.cs     diff=csharp
*.sln    merge=union
*.csproj merge=union
*.vbproj merge=union
*.fsproj merge=union
*.dbproj merge=union
